FUNCTION "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EPROFVALSTAT_TRANSF_TT_TO" (IN x_MANDT NVARCHAR(000003),x_PROFILE NVARCHAR(000018),x_DATETO NVARCHAR(000008),x_TIMETO NVARCHAR(000006),x_STAT NVARCHAR(000005),x_From TIMESTAMP)
  RETURNS y_To TIMESTAMP
  LANGUAGE SQLSCRIPT
  SQL SECURITY INVOKER AS
BEGIN
  SELECT MIN("TST") INTO y_To FROM "OTEBW_TEST_CLT_CD"."EPROFVALSTAT_CHG" WHERE MANDT=:x_MANDT AND PROFILE=:x_PROFILE AND DATETO=:x_DATETO AND TIMETO=:x_TIMETO AND STAT=:x_STAT AND TST>:x_From;
  IF y_To IS NULL THEN
    y_To = TO_TIMESTAMP('9999-12-31 23:59:59.999', 'YYYY-MM-DD HH24:MI:SS.FF3');
  END IF;
END;