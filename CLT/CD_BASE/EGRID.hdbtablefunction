FUNCTION "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EGRID" ( )
RETURNS TABLE(
MANDT NVARCHAR(000003),GRID_ID NVARCHAR(000010),SUP_GRID_ID NVARCHAR(000010),SPARTYP NVARCHAR(000002),GRID_LEVEL_TYPE NVARCHAR(000004),EXTERNALID NVARCHAR(000040),EXT_CODELISTID NVARCHAR(000003),TT_FROM TIMESTAMP,TT_TO TIMESTAMP)
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT MANDT,GRID_ID,SUP_GRID_ID,SPARTYP,GRID_LEVEL_TYPE,EXTERNALID,EXT_CODELISTID,TST AS TT_FROM, "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EGRID_TRANSF_TT_TO"(MANDT,GRID_ID,TST) as TT_TO from "OTEBW_TEST_CLT_CD"."EGRID_CHG" where (not CLT_ACTION='D');
END;