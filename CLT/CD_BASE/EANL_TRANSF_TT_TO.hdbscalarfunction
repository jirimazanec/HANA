FUNCTION "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EANL_TRANSF_TT_TO" (IN x_Mandt VARCHAR(3), x_Anlage VARCHAR(10), x_From TIMESTAMP) 
	RETURNS y_To TIMESTAMP
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN
  SELECT MIN("TST") INTO y_To FROM "OTEBW_TEST_CLT_CD"."EANL_CHG" WHERE MANDT = :x_Mandt AND ANLAGE = :x_Anlage AND TST > :x_From;
  IF y_To IS NULL THEN
    y_To = TO_TIMESTAMP('9999-12-31 23:59:59.999', 'YYYY-MM-DD HH24:MI:SS.FF3');
  END IF;
END;