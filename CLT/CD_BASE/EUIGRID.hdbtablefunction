FUNCTION "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EUIGRID" ( )
RETURNS TABLE(
MANDT NVARCHAR(000003),INT_UI NVARCHAR(000022),DATETO NVARCHAR(000008),TIMETO NVARCHAR(000006),DATEFROM NVARCHAR(000008),TIMEFROM NVARCHAR(000006),GRID_ID NVARCHAR(000010),ERDAT NVARCHAR(000008),ERNAM NVARCHAR(000012),AEDAT NVARCHAR(000008),AENAM NVARCHAR(000012),BEGRU NVARCHAR(000004),LOEVM NVARCHAR(000001),GRID_LEVEL NVARCHAR(000010),TT_FROM TIMESTAMP,TT_TO TIMESTAMP)
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT MANDT,INT_UI,to_date(nullif(DATETO,'00000000'),'YYYYMMDD') as DATETO,to_time(nullif(TIMETO,'000000'),'HHMMSS') as TIMETO,to_date(nullif(DATEFROM,'00000000'),'YYYYMMDD') as DATEFROM,to_time(nullif(TIMEFROM,'000000'),'HHMMSS') as TIMEFROM,GRID_ID,to_date(nullif(ERDAT,'00000000'),'YYYYMMDD') as ERDAT,ERNAM,to_date(nullif(AEDAT,'00000000'),'YYYYMMDD') as AEDAT,AENAM,BEGRU,LOEVM,GRID_LEVEL,TST AS TT_FROM, "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EUIGRID_TRANSF_TT_TO"(MANDT,INT_UI,DATETO,TIMETO,TST) as TT_TO from "OTEBW_TEST_CLT_CD"."EUIGRID_CHG" where (not CLT_ACTION='D');
END;