FUNCTION "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EANL" ( )
RETURNS TABLE(
MANDT NVARCHAR(000003),ANLAGE NVARCHAR(000010),SPARTE NVARCHAR(000002),VSTELLE NVARCHAR(000010),ABLSPERR NVARCHAR(000002),BAPERTYP NVARCHAR(000001),ANSCHREI NVARCHAR(000010),SPEBENE NVARCHAR(000002),DRCKSTUF NVARCHAR(000002),ANLART NVARCHAR(000004),BEZUG NVARCHAR(000001),ABLESARTST NVARCHAR(000008),NODISCONCT NVARCHAR(000004),SERVICE NVARCHAR(000004),DEREGSTAT NVARCHAR(000002),INFOREL NVARCHAR(000002),ETIMEZONE NVARCHAR(000006),OUCONT NVARCHAR(000001),HOLICALID NVARCHAR(000002),ERDAT NVARCHAR(000008),ERNAM NVARCHAR(000012),AEDAT NVARCHAR(000008),AENAM NVARCHAR(000012),BEGRU NVARCHAR(000004),LOEVM NVARCHAR(000001),TT_FROM TIMESTAMP,TT_TO TIMESTAMP)
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT MANDT,ANLAGE,SPARTE,VSTELLE,ABLSPERR,BAPERTYP,ANSCHREI,SPEBENE,DRCKSTUF,ANLART,BEZUG,ABLESARTST,NODISCONCT,SERVICE,DEREGSTAT,INFOREL,ETIMEZONE,OUCONT,HOLICALID,to_date(nullif(ERDAT,'00000000'),'YYYYMMDD') as ERDAT,ERNAM,to_date(nullif(AEDAT,'00000000'),'YYYYMMDD') as AEDAT,AENAM,BEGRU,LOEVM,TST AS TT_FROM, "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EANL_TRANSF_TT_TO"(MANDT,ANLAGE,TST) as TT_TO from "OTEBW_TEST_CLT_CD"."EANL_CHG" where (not CLT_ACTION='D');
END;