FUNCTION "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EUITRANS" ( )
RETURNS TABLE(
MANDT NVARCHAR(000003),INT_UI NVARCHAR(000022),DATETO NVARCHAR(000008),TIMETO NVARCHAR(000006),DATEFROM NVARCHAR(000008),TIMEFROM NVARCHAR(000006),EXT_UI NVARCHAR(000050),UISTRUTYP NVARCHAR(000002),ERDAT NVARCHAR(000008),ERNAM NVARCHAR(000012),AEDAT NVARCHAR(000008),AENAM NVARCHAR(000012),BEGRU NVARCHAR(000004),LOEVM NVARCHAR(000001),TT_FROM TIMESTAMP,TT_TO TIMESTAMP)
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT MANDT,INT_UI,to_date(nullif(DATETO,'00000000'),'YYYYMMDD') as DATETO,to_time(nullif(TIMETO,'000000'),'HHMMSS') as TIMETO,to_date(nullif(DATEFROM,'00000000'),'YYYYMMDD') as DATEFROM,to_time(nullif(TIMEFROM,'000000'),'HHMMSS') as TIMEFROM,EXT_UI,UISTRUTYP,to_date(nullif(ERDAT,'00000000'),'YYYYMMDD') as ERDAT,ERNAM,to_date(nullif(AEDAT,'00000000'),'YYYYMMDD') as AEDAT,AENAM,BEGRU,LOEVM,TST AS TT_FROM, "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EUITRANS_TRANSF_TT_TO"(MANDT,INT_UI,DATETO,TIMETO,TST) as TT_TO from "OTEBW_TEST_CLT_CD"."EUITRANS_CHG" where (not CLT_ACTION='D');
END;