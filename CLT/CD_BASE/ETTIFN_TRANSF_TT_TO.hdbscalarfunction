FUNCTION "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::ETTIFN_TRANSF_TT_TO" (IN x_Mandt NVARCHAR(3), x_Anlage NVARCHAR(10), x_Operand NVARCHAR(10), x_Saison NVARCHAR(10), x_Ab date, x_AblfdNr NVARCHAR(4), x_From TIMESTAMP) 
	RETURNS y_To TIMESTAMP
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN
  SELECT MIN("TST") INTO y_To FROM "OTEBW_TEST_CLT_CD"."ETTIFN_CHG" WHERE MANDT = :x_Mandt AND ANLAGE = :x_Anlage AND OPERAND = :x_Operand AND SAISON = :x_Saison AND AB = :x_Ab AND ABLFDNR = :x_AblfdNr AND TST > :x_From;
  IF y_To IS NULL THEN
    y_To = TO_TIMESTAMP('9999-12-31 23:59:59.999', 'YYYY-MM-DD HH24:MI:SS.FF3');
  END IF;
END;