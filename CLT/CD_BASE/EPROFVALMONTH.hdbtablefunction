FUNCTION "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EPROFVALMONTH" ( )
RETURNS TABLE(
MANDT NVARCHAR(000003),PROFILE NVARCHAR(000018),VALUEDAY NVARCHAR(000008),VAL01 DECIMAL(000016,000000),VAL02 DECIMAL(000016,000000),VAL03 DECIMAL(000016,000000),VAL04 DECIMAL(000016,000000),VAL05 DECIMAL(000016,000000),VAL06 DECIMAL(000016,000000),VAL07 DECIMAL(000016,000000),VAL08 DECIMAL(000016,000000),VAL09 DECIMAL(000016,000000),VAL10 DECIMAL(000016,000000),VAL11 DECIMAL(000016,000000),VAL12 DECIMAL(000016,000000),VAL13 DECIMAL(000016,000000),VAL14 DECIMAL(000016,000000),VAL15 DECIMAL(000016,000000),VAL16 DECIMAL(000016,000000),VAL17 DECIMAL(000016,000000),VAL18 DECIMAL(000016,000000),VAL19 DECIMAL(000016,000000),VAL20 DECIMAL(000016,000000),VAL21 DECIMAL(000016,000000),VAL22 DECIMAL(000016,000000),VAL23 DECIMAL(000016,000000),VAL24 DECIMAL(000016,000000),VAL25 DECIMAL(000016,000000),VAL26 DECIMAL(000016,000000),VAL27 DECIMAL(000016,000000),VAL28 DECIMAL(000016,000000),VAL29 DECIMAL(000016,000000),VAL30 DECIMAL(000016,000000),VAL31 DECIMAL(000016,000000),TT_FROM TIMESTAMP,TT_TO TIMESTAMP)
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT MANDT,PROFILE,to_date(nullif(VALUEDAY,'00000000'),'YYYYMMDD') as VALUEDAY,VAL01,VAL02,VAL03,VAL04,VAL05,VAL06,VAL07,VAL08,VAL09,VAL10,VAL11,VAL12,VAL13,VAL14,VAL15,VAL16,VAL17,VAL18,VAL19,VAL20,VAL21,VAL22,VAL23,VAL24,VAL25,VAL26,VAL27,VAL28,VAL29,VAL30,VAL31,TST AS TT_FROM, "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EPROFVALMONTH_TRANSF_TT_TO"(MANDT,PROFILE,VALUEDAY,TST) as TT_TO from "OTEBW_TEST_CLT_CD"."EPROFVALMONTH_CHG" where (not CLT_ACTION='D');
END;