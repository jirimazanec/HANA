FUNCTION "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EPROFVALSTAT" ( )
RETURNS TABLE(
MANDT NVARCHAR(000003),PROFILE NVARCHAR(000018),DATETO NVARCHAR(000008),TIMETO NVARCHAR(000006),STAT NVARCHAR(000005),DATEFROM NVARCHAR(000008),TIMEFROM NVARCHAR(000006),INACT NVARCHAR(000001),CHGNR NVARCHAR(000003),TT_FROM TIMESTAMP,TT_TO TIMESTAMP)
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT MANDT,PROFILE,to_date(nullif(DATETO,'00000000'),'YYYYMMDD') as DATETO,to_time(nullif(TIMETO,'000000'),'HHMMSS') as TIMETO,STAT,to_date(nullif(DATEFROM,'00000000'),'YYYYMMDD') as DATEFROM,to_time(nullif(TIMEFROM,'000000'),'HHMMSS') as TIMEFROM,INACT,CHGNR,TST AS TT_FROM, "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EPROFVALSTAT_TRANSF_TT_TO"(MANDT,PROFILE,DATETO,TIMETO,STAT,TST) as TT_TO from "OTEBW_TEST_CLT_CD"."EPROFVALSTAT_CHG" where (not CLT_ACTION='D');
END;