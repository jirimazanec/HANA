FUNCTION "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EABLG_TRANSF_TT_TO" (IN x_MANDT NVARCHAR(000003),x_ABLBELNR NVARCHAR(000020),x_ANLAGE NVARCHAR(000010),x_ABLESGR NVARCHAR(000002),x_From TIMESTAMP)
  RETURNS y_To TIMESTAMP
  LANGUAGE SQLSCRIPT
  SQL SECURITY INVOKER AS
BEGIN
  SELECT MIN("TST") INTO y_To FROM "OTEBW_TEST_CLT_CD"."EABLG_CHG" WHERE MANDT=:x_MANDT AND ABLBELNR=:x_ABLBELNR AND ANLAGE=:x_ANLAGE AND ABLESGR=:x_ABLESGR AND TST>:x_From;
  IF y_To IS NULL THEN
    y_To = TO_TIMESTAMP('9999-12-31 23:59:59.999', 'YYYY-MM-DD HH24:MI:SS.FF3');
  END IF;
END;