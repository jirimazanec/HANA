FUNCTION "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EGRIDT" ( )
RETURNS TABLE(
MANDT NVARCHAR(000003),SPRAS NVARCHAR(000001),GRID_ID NVARCHAR(000010),GRID_NAME NVARCHAR(000040),TT_FROM TIMESTAMP,TT_TO TIMESTAMP)
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT MANDT,SPRAS,GRID_ID,GRID_NAME,TST AS TT_FROM, "OTEBW_TEST_CLT_CD"."CLT.CD_BASE::EGRIDT_TRANSF_TT_TO"(MANDT,SPRAS,GRID_ID,TST) as TT_TO from "OTEBW_TEST_CLT_CD"."EGRIDT_CHG" where (not CLT_ACTION='D');
END;