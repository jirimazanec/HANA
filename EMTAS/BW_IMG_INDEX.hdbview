schema="OTEBW_EMTAS";

query = "select 
to_date(DDATE) as DDATE,
TYPE,
PRICE,
CURRENCY,
SYSTEMTS,
T.CALMONTH as CALMONTH,
T.YEAR as CALYEAR,
T.CALWEEK as CALWEEK,
T.CALQUARTER as CALQUARTER
from BW_IMG_INDEX,
     _SYS_BI.TC_DAY_CALENDAR T
where T.DATE_SQL = DDATE  ";



depends_on_table=["OTEBW_EMTAS::BW_IMG_INDEX"];