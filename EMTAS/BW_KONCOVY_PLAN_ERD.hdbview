schema="OTEBW_EMTAS";

query = "SELECT 

to_date(DELIVERYDAY) as DELIVERYDAY,
VERSION,
EXTERNALVERSION,
CALCULATIONID,
PERIOD,
PARTIC,
BUYSELL,
BUSSTYPE,
VOLUME,
T.CALMONTH as CALMONTH,
T.YEAR as CALYEAR,
T.CALWEEK as CALWEEK,
T.CALQUARTER as CALQUARTER, 
SYSTEMTS,
'MWH' as UNIT
from BW_KONCOVY_PLAN_ERD,
     _SYS_BI.TC_DAY_CALENDAR T
where T.DATE_SQL = DELIVERYDAY
  and version in (2,3,4)";

