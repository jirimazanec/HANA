FUNCTION "OTEBW_CD"."CD::1E_EUITRANS"(in x_TST timestamp)
RETURNS TABLE(
INT_UI NVARCHAR(000022),DATETO NVARCHAR(000008),DATEFROM NVARCHAR(000008),EXT_UI NVARCHAR(000050))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
INT_UI,DATETO,DATEFROM,EXT_UI
from "OTEBW_CD"."EUITRANS_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST and LOEVM=char(32)
with hint(INLINE);
END;