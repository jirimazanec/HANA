FUNCTION "OTEBW_CD"."CD::1E_EPROFVAL60"(in x_TST timestamp)
RETURNS TABLE(
PROFILE NVARCHAR(000018),VALUEDAY NVARCHAR(000008),VAL0000 DECIMAL(000016,000000),VAL0100 DECIMAL(000016,000000),VAL0200 DECIMAL(000016,000000),VAL0300 DECIMAL(000016,000000),VAL0400 DECIMAL(000016,000000),VAL0500 DECIMAL(000016,000000),VAL0600 DECIMAL(000016,000000),VAL0700 DECIMAL(000016,000000),VAL0800 DECIMAL(000016,000000),VAL0900 DECIMAL(000016,000000),VAL1000 DECIMAL(000016,000000),VAL1100 DECIMAL(000016,000000),VAL1200 DECIMAL(000016,000000),VAL1300 DECIMAL(000016,000000),VAL1400 DECIMAL(000016,000000),VAL1500 DECIMAL(000016,000000),VAL1600 DECIMAL(000016,000000),VAL1700 DECIMAL(000016,000000),VAL1800 DECIMAL(000016,000000),VAL1900 DECIMAL(000016,000000),VAL2000 DECIMAL(000016,000000),VAL2100 DECIMAL(000016,000000),VAL2200 DECIMAL(000016,000000),VAL2300 DECIMAL(000016,000000))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
PROFILE,VALUEDAY,VAL0000,VAL0100,VAL0200,VAL0300,VAL0400,VAL0500,VAL0600,VAL0700,VAL0800,VAL0900,VAL1000,VAL1100,VAL1200,VAL1300,VAL1400,VAL1500,VAL1600,VAL1700,VAL1800,VAL1900,VAL2000,VAL2100,VAL2200,VAL2300
from "OTEBW_CD"."EPROFVAL60_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST
with hint(INLINE);
END;