FUNCTION "OTEBW_CD"."CD::2G_GetProfValStat"(x_TST timestamp, x_Profile nvarchar(18), x_ValDay nvarchar(8), x_ValTime nvarchar(6)) 
	RETURNS y_Stat nvarchar(2)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN
select ifnull("OTEBW_CD"."CD::ConvertProfStat"(STAT), '00') into y_Stat from "OTEBW_CD"."CD::1G_EPROFVALSTAT"(:x_TST)
                 where PROFILE=:x_Profile and (DATEFROM<:x_ValDay or (DATEFROM=:x_ValDay and TIMEFROM<=:x_ValTime))
                                          and (DATETO>:x_ValDay or (DATETO=:x_ValDay and TIMETO>=:x_ValTime)) with hint(INLINE);
END;