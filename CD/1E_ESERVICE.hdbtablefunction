FUNCTION "OTEBW_CD"."CD::1E_ESERVICE"(in x_TST timestamp)
RETURNS TABLE(
INT_UI NVARCHAR(000022),SERVICE_START NVARCHAR(000008),SERVICE_END NVARCHAR(000008),SERVICE NVARCHAR(000004),SERVICEID NVARCHAR(000010),SRVPRVREF NVARCHAR(000030))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
INT_UI,SERVICE_START,SERVICE_END,SERVICE,SERVICEID,SRVPRVREF
from "OTEBW_CD"."ESERVICE_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST and LOEVM=char(32)
with hint(INLINE);
END;