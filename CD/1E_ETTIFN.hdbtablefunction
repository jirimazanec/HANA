FUNCTION "OTEBW_CD"."CD::1E_ETTIFN"(in x_TST timestamp)
RETURNS TABLE(
ANLAGE NVARCHAR(000010),OPERAND NVARCHAR(000010),AB NVARCHAR(000008),BIS NVARCHAR(000008),WERT1 DECIMAL(000016,000007),WERT2 DECIMAL(000016,000007),STRING1 NVARCHAR(000010),STRING2 NVARCHAR(000010),STRING3 NVARCHAR(000001),STRING4 NVARCHAR(000001))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
ANLAGE,OPERAND,AB,BIS,WERT1,WERT2,STRING1,STRING2,STRING3,STRING4
from "OTEBW_CD"."ETTIFN_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST
with hint(INLINE);
END;