FUNCTION "OTEBW_CD"."CD::1E_EABL"(in x_TST timestamp)
RETURNS TABLE(
ABLBELNR NVARCHAR(000020),ZWNUMMER NVARCHAR(000003),ADAT NVARCHAR(000008),ATIM NVARCHAR(000004),V_ZWSTAND DECIMAL(000017,000000),N_ZWSTAND DECIMAL(000014,000014),MASSREAD NVARCHAR(000003),MASSBILL NVARCHAR(000003),STANZVOR NVARCHAR(000002),STANZNAC NVARCHAR(000002),ABLESER NVARCHAR(000003))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
ABLBELNR,ZWNUMMER,ADAT,ATIM,V_ZWSTAND,N_ZWSTAND,MASSREAD,MASSBILL,STANZVOR,STANZNAC,ABLESER
from "OTEBW_CD"."EABL_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST
with hint(INLINE);
END;