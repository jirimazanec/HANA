FUNCTION "OTEBW_CD"."CD::1E_EABLG"(in x_TST timestamp)
RETURNS TABLE(
ABLBELNR NVARCHAR(000020),ANLAGE NVARCHAR(000010),ABLESGR NVARCHAR(000002))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
ABLBELNR,ANLAGE,ABLESGR
from "OTEBW_CD"."EABLG_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST
with hint(INLINE);
END;