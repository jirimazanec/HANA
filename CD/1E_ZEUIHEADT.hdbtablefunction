FUNCTION "OTEBW_CD"."CD::1E_ZEUIHEADT"(in x_TST timestamp)
RETURNS TABLE(
INT_UI NVARCHAR(000022),SPRAS NVARCHAR(000001),AB NVARCHAR(000008),BIS NVARCHAR(000008),UITEXT NVARCHAR(000060))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
INT_UI,SPRAS,AB,BIS,UITEXT
from "OTEBW_CD"."ZEUIHEADT_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST
with hint(INLINE);
END;