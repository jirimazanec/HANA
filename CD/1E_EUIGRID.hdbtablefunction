FUNCTION "OTEBW_CD"."CD::1E_EUIGRID"(in x_TST timestamp)
RETURNS TABLE(
INT_UI NVARCHAR(000022),DATETO NVARCHAR(000008),DATEFROM NVARCHAR(000008),GRID_ID NVARCHAR(000010))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
INT_UI,DATETO,DATEFROM,GRID_ID
from "OTEBW_CD"."EUIGRID_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST and LOEVM=char(32)
with hint(INLINE);
END;