FUNCTION "OTEBW_CD"."CD::1E_EANL"(in x_TST timestamp)
RETURNS TABLE(
ANLAGE NVARCHAR(000010),VSTELLE NVARCHAR(000010),SPEBENE NVARCHAR(000002),ANLART NVARCHAR(000004))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
ANLAGE,VSTELLE,SPEBENE,ANLART
from "OTEBW_CD"."EANL_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST and LOEVM=char(32)
with hint(INLINE);
END;