FUNCTION "OTEBW_CD"."CD::1E_EPROFHEAD"(in x_TST timestamp)
RETURNS TABLE(
PROFILE NVARCHAR(000018),PROFTYPE NVARCHAR(000002),PROFVALCAT NVARCHAR(000002),INTSIZEID NVARCHAR(000004),MASS NVARCHAR(000003),WAERS NVARCHAR(000005),PROFDECIMALS NVARCHAR(000002))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
PROFILE,PROFTYPE,PROFVALCAT,INTSIZEID,MASS,WAERS,PROFDECIMALS
from "OTEBW_CD"."EPROFHEAD_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST and not (LOEVM='X')
with hint(INLINE);
END;