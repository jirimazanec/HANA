FUNCTION "OTEBW_CD"."CD::1E_EUIINSTLN"(in x_TST timestamp)
RETURNS TABLE(
INT_UI NVARCHAR(000022),ANLAGE NVARCHAR(000010),DATETO NVARCHAR(000008),DATEFROM NVARCHAR(000008))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
INT_UI,ANLAGE,DATETO,DATEFROM
from "OTEBW_CD"."EUIINSTLN_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST and LOEVM=char(32);
END;