FUNCTION "OTEBW_CD"."CD::1E_ELPASS"(in x_TST timestamp)
RETURNS TABLE(
OBJKEY NVARCHAR(000010),BIS NVARCHAR(000008),AB NVARCHAR(000008),PROFILE NVARCHAR(000018),PROFROLE NVARCHAR(000004))
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER AS BEGIN
RETURN SELECT
left(OBJKEY,10) as OBJKEY,BIS,AB,PROFILE,PROFROLE
from "OTEBW_CD"."ELPASS_CHG" where MANDT='100' and (not CLT_ACTION='D') and CLT_TST<=:x_TST and CLT_TST_TO>=:x_TST and OBJTYPE='INSTLN'
with hint(INLINE);
END;