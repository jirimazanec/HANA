      <attribute id="PPS" order="24">
        <descriptions defaultDescription="PPS"/>
        <keyMapping columnObjectName="Script_View" columnName="PPS"/>
      </attribute>
    </attributes>
    <calculatedAttributes/>
    <privateDataFoundation>
