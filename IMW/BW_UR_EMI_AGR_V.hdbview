schema="OTEBW_IMW";

query="SELECT
	     VERIFIED_ENTITY_ID as VER_ENTITY_ID,
	     PERIOD as PERIOD,
	     SUM( CO2 ) as SUMCO2,
	     DTYPE as DTYPE,
	     SUM( N2O ) as SUMN2O,
	     SUM( PFC ) as SUMPFC
	   FROM OTEBW_IMW.REMOTE_BW_UR_EMI_V
	   WHERE VERIFIED = '1'
	   GROUP BY VERIFIED_ENTITY_ID, PERIOD, DTYPE";