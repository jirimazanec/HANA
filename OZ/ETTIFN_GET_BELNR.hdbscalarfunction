FUNCTION "OTEBW_OZ"."OZ::ETTIFN_GET_BELNR" (X_ANLAGE nvarchar(10), X_AB date, X_BIS date ) 
	RETURNS Y_BELNR nvarchar(12)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "OTEBW_OZ"
	DETERMINISTIC
	AS
BEGIN

SELECT MAX(BELNR) INTO Y_BELNR FROM OTEBW_OZ.ETTIFN WHERE MANDT = '400' AND ANLAGE = :X_ANLAGE AND AB <= :X_AB AND BIS >= :X_BIS;

END;