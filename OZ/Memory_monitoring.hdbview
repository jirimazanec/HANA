schema="OTEBW_OZ";

query = "select top 1000 HOST,
	 	SERVER_TIMESTAMP,
	 	round(INSTANCE_TOTAL_MEMORY_USED_SIZE/(1024*1024*1024), 2) as \"Peak Used GB\" 
from _SYS_STATISTICS.HOST_RESOURCE_UTILIZATION_STATISTICS 
Where SERVER_TIMESTAMP BETWEEN ADD_DAYS(NOW(),	 -2) AND NOW() 
order by \"Peak Used GB\" desc , \"SERVER_TIMESTAMP\" desc"; 
